
{{#each listaProductos}}
<div class="col-sm-4 mb-3">
  <div class="card">
    <img src="{{this.thumbnail}}" class="card-img-top" alt="{{this.title}}">
    <div class="card-body">
      <h5 class="card-title">{{ this.title }}</h5>
      <p class="card-text">{{this.description}}</p>
      <p>{{this.price}}$</p>
      <a href="#" class="btn btn-primary">Agregar al carrito</a>
    </div>
  </div>
</div>
{{/each}}